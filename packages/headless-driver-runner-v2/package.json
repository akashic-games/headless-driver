{
  "name": "@akashic/headless-driver-runner-v2",
  "version": "1.9.6",
  "description": "A runner module for contents using Akashic Engine v2",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "rimraf ./lib && tsc -p ./tsconfig.json && npm run formatter",
    "formatter": "prettier --write \"src/**/*.ts\" --config ../prettier.config.js",
    "test": "npm run lint",
    "lint": "eslint \"src/**/*.ts\" --fix"
  },
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  },
  "files": [
    "lib"
  ],
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "typings": "lib/index.d.ts",
  "dependencies": {
    "@akashic/engine-files": "2.1.57",
    "@akashic/headless-driver-runner": "1.9.6"
  },
  "devDependencies": {
    "@akashic/eslint-config": "1.0.0",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "eslint": "8.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jest": "25.2.2",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "typescript": "4.4.3"
  }
}
