{
  "name": "@akashic/headless-driver-runner-v1",
  "version": "1.11.9",
  "description": "A runner module for contents using Akashic Engine v1",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "rimraf ./lib && tsc -p ./tsconfig.json && npm run formatter",
    "formatter": "prettier --write \"src/**/*.ts\" --config ../prettier.config.js",
    "test": "npm run lint",
    "lint": "eslint \"src/**/*.ts\" --fix"
  },
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  },
  "files": [
    "lib"
  ],
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "typings": "lib/index.d.ts",
  "dependencies": {
    "@akashic/engine-files": "1.1.16",
    "@akashic/headless-driver-runner": "1.11.9"
  },
  "devDependencies": {
    "@akashic/eslint-config": "1.0.0",
    "@typescript-eslint/eslint-plugin": "5.5.0",
    "eslint": "8.4.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.3.0",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "typescript": "4.5.2"
  }
}
